                    <TextBlock Canvas.Right="25" FontSize="50" Foreground="White">HP</TextBlock>
                    <TextBlock Canvas.Left="750" FontSize="50" Foreground="White">OCHRONIARZ</TextBlock>

                    <Image Source="/mc.gif" Canvas.Bottom="50"/>
